/* ------------------------------------------------------------------------ *\
   mixins.hack
\* ------------------------------------------------------------------------ */


ie = nix.legacy.ie
legacy = nix.legacy.supported


/**
 * [ie description]
 * @return {[type]} [description]
 */
h-global--ie()
  if (ie == true)
    if(ie == true)
      if (version == legacy)
        {block}
    else
      if (version <= legacy)
        {block}
